<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mathflash">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/refine_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/refine_icon_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MathFlash">
        <activity android:name=".practice_end_screen"></activity>
        <activity
            android:name=".SettingsActivity"
            android:label="@string/settings" />
        <activity android:name=".end_screen" />
        <activity android:name=".wrong" />
        <activity android:name=".correct" />
        <activity android:name=".game" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>